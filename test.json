MATCH (a:age)-[:REQUIRES]->(s:supplement)
MATCH (g:gender)-[:HIGHER_NEED]->(s)
MATCH (d:diet)-[:MAY_LACK]->(s)
MATCH (s)-[:MANAGES]->(sym:symptom)
MATCH (s)-[:MANAGES]->(sym2:symptom)
WHERE toLower(a.name) CONTAINS 'middle-aged'
AND toLower(g.name) CONTAINS 'female'
AND toLower(d.name) CONTAINS 'vegan'
AND toLower(sym.name) CONTAINS 'fatigue'
AND toLower(sym2.name) CONTAINS 'iron deficiency'
RETURN DISTINCT s.name AS supplement
LIMIT 10

MATCH (gender:gender)-[:HIGHER_NEED]->(s:supplement)
MATCH (s)-[:MANAGES]->(symptom:symptom)
MATCH (bmi:body_mass_index)-[:MAY_NEED]->(s)
WHERE toLower(gender.name) CONTAINS 'female'
AND toLower(symptom.name) CONTAINS 'headache'
AND toLower(bmi.name) CONTAINS 'overweight'
RETURN DISTINCT s.name AS supplement
LIMIT 10

MATCH (p:Parameter)-[:ASSOCIATED_WITH]->(v:Parameter_Value)
WHERE p.name CONTAINS 'Heart Rate' AND v.name CONTAINS 'High'
MATCH (v)-[:NEGATIVELY_AFFECT]->(m:Health_Condition_Medical)
MATCH (m)-[:RECOMMENDED_LIFESTYLE_CHANGES]->(l:Lifestyle)
RETURN DISTINCT p.name AS parameter, v.name AS value, m.name AS condition, l.name AS recommended_lifestyle
LIMIT 10


